{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "cloudplayplus_stone",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Flutter (Web Security Disabled)",
            "request": "launch",
            "type": "dart",
            "toolArgs": ["--web-browser-flag","--disable-web-security"]
        },
        {
            "name": "Flutter (local engine)",
            "request": "launch",
            "type": "dart",
            "toolArgs": ["--web-browser-flag","--disable-web-security","--local-web-sdk=wasm_release"]
        },
        {
            "name": "Debug Native",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/linux/x64/debug/bundle/cloudplayplus",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/linux/x64/debug/bundle",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
            {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
            }
            ]
        },
        {
            "name": "cloudplayplus_stone (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "cloudplayplus_stone (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "flutter_login",
            "cwd": "plugins/flutter_login",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_login (profile mode)",
            "cwd": "plugins/flutter_login",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_login (release mode)",
            "cwd": "plugins/flutter_login",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "example",
            "cwd": "plugins/flutter_login/example",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "example (profile mode)",
            "cwd": "plugins/flutter_login/example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "example (release mode)",
            "cwd": "plugins/flutter_login/example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}